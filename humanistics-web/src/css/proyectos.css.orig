.list-proyectos{
    width: 100%;
}

.list-proyectos .ant-list-items .ant-list-item{
    padding: 2px !important;
}

.list-proyectos .card-list{
    width: 100%;
    box-shadow: 4px 5px 15px -1px rgb(230 230 230);
    border-radius: 10px;
}

.list-proyectos .ant-card-body {
    padding: 12px 10px;
}

.list-proyectos .ant-typography-secondary {

font-style: normal;
font-weight: 500;
font-size: 9.5pt;
line-height: 15px;
letter-spacing: 0.1px;
color: #6084b3;
}

.row-list-proyectos{
     max-height: 26vh;
     overflow: auto;
     margin-bottom: 10px;
     
}


.card-proyecto  {
    box-shadow: 4px 5px 15px -1px rgb(230 230 230);
}



@media (min-width: 768px) {
    .row-list-proyectos{
         max-height: 45vh !important;
    }
}


@media (min-width: 1200px) {
    .row-list-proyectos{
         max-height: 63vh !important;
    }
}


.row-cards-proyectos{
     display: inline-block;
 
}

.row-cards-proyectos .component-card-item
 {
     margin: 10px;
     width: auto !important;
     display: inline-flex;
     position: relative;
     padding: 5px;
     
}

.card-proyecto .ant-card-head-title span{
    width: 100% !important;
    display: block;
    margin-top: 10px;
    font-size: 15pt;

}

.card-proyecto .ant-card-head-title .ant-avatar{
    height: 80px !important;
    width:  80px !important;
    text-align: center;
    margin-left: 30% !important;
}

.card-proyecto {
    height: 480px !important;
    position: relative; 
    min-width: 300px;
}

.card-proyecto  .ant-card-body{
    position: relative;
    overflow: auto;
    padding: 5px 10px;
    max-height: 300px;
    border: solid 1px rgba(0, 0, 0, 0.048);


}


.row-info-proyecto {
    height: 130px;
    overflow: auto;
    margin-bottom: 5px;
}
.row-list-estatus {
    max-height: 150px;
    overflow: auto;
}

.row-list-estatus  .box-prospectos-color{
    height:     30px !important;
    width:      30px !important;
    max-width:  30px !important;
    min-height: 30px !important;
    min-width:  30px !important;
    max-height: 30px !important;
}


.btns-proyectos button {
    padding: 0px !important;
    text-align: center !important;
}

.btns-proyectos button svg{
    font-size: 12pt !important;
}

.btns-proyectos button .anticon-appstore{
    font-size: 15pt !important;
    margin-left: 0px !important;
    margin-top: 5px  !important;
}

.btns-proyectos button .anticon-appstore svg{
    font-size: 15pt !important;
}

.layout-proyectos .cnt-proyectos .ant-page-header-heading-title{
    padding-left: 22px;
}


/*Proyecto*/

.cnt-proyecto  .ant-spin-container{
    padding: 10px 25px;
}


.cnt-proyecto .header-container{
    padding:15px !important;
}


.cnt-proyecto .ant-page-header-heading-title{
    font-style: normal;
    font-weight: bolder;
    font-size: 22px;
    line-height: 25px;
    color: #636D73;
    letter-spacing: 1px !important;
}


.cnt-proyecto .header-container .ant-typography{
    float: left ;
    width: 100% !important;
    text-align: center !important;

    font-style: normal;
    letter-spacing: 0.225px;
    color: #7E97B8;
}


.cnt-proyecto .header-container h1
{
    font-weight: bold;
    font-size: 15px;
    line-height: 21px;
}


.cnt-proyecto .header-container span
{
    font-weight: normal;
    font-size: 12px;
    line-height: 15px;
}

.cnt-proyecto .list-prospectos,
.cnt-proyecto .list-prospectos .ant-spin-container{
    width: 100% !important;
    padding: 0px 10px 10px 0px !important;
}

.cnt-proyecto  .list-prospectos .ant-list-items{
    max-height: 50vh !important;
    overflow: auto;
}


.cnt-proyecto  .list-prospectos .ant-list-item {
    padding: 5px 15px 10px 3px !important;
}

.cnt-proyecto .list-prospectos .card-list {
    box-shadow: -4px -2px 16px #FFFFFF, 4px 2px 16px rgba(95, 157, 231, 0.48);
}


.cnt-proyecto .cnt-list-prospectos {
    position: relative;
    min-width: 600px !important;
    display: inline-block;
    width: 100%;
}


 
.layout-proyectos .ant-page-header-heading-extra .purple-icon:hover,
.layout-proyectos .ant-page-header-heading-extra .purple-icon:focus,
.layout-proyectos .ant-page-header-heading-extra .purple-icon:active,
.cnt-proyecto .cnt-list-prospectos .purple-icon:hover,
.cnt-proyecto .cnt-list-prospectos .purple-icon:focus,
.cnt-proyecto .cnt-list-prospectos .purple-icon:active
 {
    background:  #492af9d3;
    color: white;
    border: 3px solid rgba(211, 204, 255, 0.5);
}



@media (min-width: 1200px) {
    .cnt-proyecto .contenedor-logo {
        float: left;
        margin-left: 10px;
    }
}

